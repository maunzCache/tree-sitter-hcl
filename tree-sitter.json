{
  "grammars": [
    {
      "name": "hcl",
      "camelcase": "Hcl",
      "scope": "source.hcl",
      "path": ".",
      "file-types": [
        "hcl"
      ]
    },
    {
      "name": "terraform",
      "camelcase": "Terraform",
      "scope": "source.terraform",
      "path": "./dialects/terraform/",
      "file-types": [
        "tf",
        "tfvars"
      ]
    }
  ],
  "metadata": {
    "version": "0.2.0-snapshot",
    "license": "Apache-2.0",
    "description": "HCL grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-hcl"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": true,
    "python": false,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
